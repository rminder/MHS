    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{A5514B87-7962-4FD2-A5B7-4F8EAF7BC7D6}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;Declare @TargetDate Date = '&quot; + (DT_WSTR,50)@[User::BeginDate] + &quot;' &#xA;Declare @RowCount int&#xA;&#xA;-- Check to see if we just processed any rows between the date contained in the parameter value and that date plus&#xA;-- one month&#xA;Select @RowCount = Count(*)&#xA;From &quot; + @[User::OdsFullTableName] + &quot;&#xA;Where LastUpdate Between @TargetDate And DateAdd(MONTH,1,@TargetDate)&#xA;&#xA;IF @RowCount = 0&#xA;   -- No we didn't process any rows. Find the first date greater than the date contained in the parametert value. This is where we&#xA;   -- will start the next iteration&#xA;   Select ISNULL(MIN(LastUpdate),'1/1/9999') as NextDate From &quot; + @[User::OdsFullTableName] + &quot; Where LastUpdate &gt; @TargetDate&#xA;ELSE&#xA;   -- Yes we did process some rows. Advance the date contained in the paraeter one month.&#xA;   Select DateAdd(MONTH,1,@TargetDate) as NextDate&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="FindNextStartDateQuery">
      <DTS:VariableValue
        DTS:DataType="8">Declare @TargetDate Date = '1/1/1900' 
Declare @RowCount int

-- Check to see if we just processed any rows between the date contained in the parameter value and that date plus
-- one month
Select @RowCount = Count(*)
From SL.SegmentDefinition
Where LastUpdate Between @TargetDate And DateAdd(MONTH,1,@TargetDate)

IF @RowCount = 0
   -- No we didn't process any rows. Find the first date greater than the date contained in the parametert value. This is where we
   -- will start the next iteration
   Select ISNULL(MIN(LastUpdate),'1/1/9999') as NextDate From SL.SegmentDefinition Where LastUpdate &gt; @TargetDate
ELSE
   -- Yes we did process some rows. Advance the date contained in the paraeter one month.
   Select DateAdd(MONTH,1,@TargetDate) as NextDate</DTS:VariableValue>
    </DTS:Variable>
