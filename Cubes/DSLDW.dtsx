                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT a.[Acct]
      ,a.[BatNbr]
      ,a.[CpnyID]
      ,a.[CuryId]
      ,a.[CuryRate]
      ,a.[CuryTranAmt]
      ,a.[DrCr]
