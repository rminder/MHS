ID, [Sub], SubSeg1, SubSeg2, tstamp)&#xA;  &#x9; SELECT Acct, FiscYr + '07',  BdgtRvsnDate, PtdBal06,  CpnyID, CuryID, FiscYr, LedgerID,  [Sub],  SubSeg1, SubSeg2, tstamp  FROM tStageGLBudget;&#xA;&#xA;INSERT INTO fGLBudget(Acct, BdgtPer, BdgtRvsnDate, BudgetAmt, CpnyID, CuryId, FiscYr, LedgerID, [Sub], SubSeg1, SubSeg2, tstamp)&#xA;  &#x9; SELECT Acct, FiscYr + '08',  BdgtRvsnDate, PtdBal07,  CpnyID, CuryID, FiscYr, LedgerID,  [Sub],  SubSeg1, SubSeg2, tstamp  FROM tStageGLBudget;&#xA;&#xA;INSERT INTO fGLBudget(Acct, BdgtPer, BdgtRvsnDate, BudgetAmt, CpnyID, CuryId, FiscYr, LedgerID, [Sub], SubSeg1, SubSeg2, tstamp)&#xA;  &#x9; SELECT Acct, FiscYr + '09',  BdgtRvsnDate, PtdBal08,  CpnyID, CuryID, FiscYr, LedgerID,  [Sub],  SubSeg1, SubSeg2, tstamp  FROM tStageGLBudget;&#xA;&#xA;INSERT INTO fGLBudget(Acct, BdgtPer, BdgtRvsnDate, BudgetAmt, CpnyID, CuryId, FiscYr, LedgerID, [Sub], SubSeg1, SubSeg2, tstamp)&#xA;  &#x9; SELECT Acct, FiscYr + '10',  BdgtRvsnDate, PtdBal09,  CpnyID, CuryID, FiscYr, LedgerID,  [Sub],  SubSeg1, SubSeg2, tstamp  FROM tStageGLBudget;&#xA;&#xA;INSERT INTO fGLBudget(Acct, BdgtPer, BdgtRvsnDate, BudgetAmt, CpnyID, CuryId, FiscYr, LedgerID, [Sub], SubSeg1, SubSeg2, tstamp)&#xA;  &#x9; SELECT Acct, FiscYr + '11',  BdgtRvsnDate, PtdBal10,  CpnyID, CuryID, FiscYr, LedgerID,  [Sub],  SubSeg1, SubSeg2, tstamp  FROM tStageGLBudget;&#xA;&#xA;INSERT INTO fGLBudget(Acct, BdgtPer, BdgtRvsnDate, BudgetAmt, CpnyID, CuryId, FiscYr, LedgerID, [Sub], SubSeg1, SubSeg2, tstamp)&#xA;  &#x9; SELECT Acct, FiscYr + '12',  BdgtRvsnDate, PtdBal11,  CpnyID, CuryID, FiscYr, LedgerID,  [Sub],  SubSeg1, SubSeg2, tstamp  FROM tStageGLBudget;&#xA;&#xA;&#xA;   If @NBRPERIODS = 13 BEGIN&#xA; &#x9;INSERT INTO fGLBudget(Acct, BdgtPer, BdgtRvsnDate, BudgetAmt, CpnyID, CuryId, FiscYr, LedgerID, [Sub], SubSeg1, SubSeg2, tstamp)&#xA;  &#x9; SELECT Acct, FiscYr + '13',  BdgtRvsnDate, PtdBal12,  CpnyID, CuryID, FiscYr, LedgerID,  [Sub],  SubSeg1, SubSeg2, tstamp  FROM tStageGLBudget&