                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">DECLARE @APDOC TABLE ([CpnyID] [varchar](100) NULL,  [VendID] [varchar](100) NULL,[RefNbr] [varchar](100) NULL, [DocDate] [date] NULL, [DueDate] [date] NULL, [DiscDate] [date] NULL,
	[DocType] [varchar](100) NULL, [DocBal] [Numeric](15, 2) NULL,   [SubAccount] [varchar](100) NULL, [ProfitCenterID] [varchar](100) NULL,
	 [LocationID] [varchar](100) NULL, [PerPost] [varchar](100) NULL, [InvcNbr] [varchar](100) NULL , [tstamp] BIGINT NULL);

INSERT INTO @APDOC ([CpnyID],  [VendID],  [RefNbr], [DocDate], [DueDate], [DiscDate], [DocType], [DocBal], [PerPost], [InvcNbr], [tstamp]) (
	SELECT APDOC.CpnyID,  APDOC.VendId, RefNbr, CONVERT(DATE, DocDate) 'DocDate',
	 CONVERT(DATE, DueDate) 'DueDate', CONVERT(DATE, DiscDate) 'DiscDate', DocType,
			CASE 
			WHEN DocType IN ('VO', 'AC')
				THEN CuryDocBal * 1
			WHEN DocType IN ('AD', 'CK', 'EP', 'HC', 'QC')
				THEN CuryDocBal * -1
			ELSE CuryDocBal * 1	END 'DocBal',
			PerPost,
			[InvcNbr],
			CONVERT(BIGINT, APDOC.tstamp) 'tstamp'
	FROM APDOC WITH(NOLOCK)
	LEFT JOIN vs_company ON APDOC.CpnyID = vs_company.CpnyID
	LEFT JOIN Vendor ON APDOC.VendId = Vendor.VendId
	WHERE RLSED = 1 AND OpenDoc = 1 AND vs_company.Active = 1 AND DocType &lt;&gt; 'VT'
	)
;
WITH CTE
AS (
	SELECT a.RefNbr, a.Sub, a.TranType, b.tstamp
	FROM APTran a  WITH(NOLOCK)
	JOIN @APDOC b ON a.Refnbr = b.RefNbr AND a.TranType = b.DocType AND a.VendId = b.VendID
	WHERE a.Sub &lt;&gt; '000000' AND a.Sub &lt;&gt; ''
	GROUP BY a.RefNbr, a.Sub, a.TranType, b.tstamp
	)
UPDATE a
SET a.SubAccount = b.Sub
FROM @APDOC a
LEFT JOIN CTE b ON a.tstamp = b.tstamp;

UPDATE @APDOC
SET SubAccount = '000000'
WHERE SubAccount IS NULL;

UPDATE @APDOC
SET SubAccount = '000000'
WHERE SubAccount = '0'

UPDATE @APDOC
SET ProfitCenterID = SubString(SubAccount, 1, 2);

UPDATE @APDOC
SET LocationID = SubString(SubAccount, 3, 4);

SELECT CpnyID, VendID,  RefNbr, DocDate, DueDate, DiscDate, DocType, DocBal,  ProfitCenterID, LocationID, PerPost, InvcNbr FROM @APDOC</property>
