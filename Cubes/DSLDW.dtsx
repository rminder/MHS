      , PtdBal11 = CASE WHEN SubString(b.accttype, 2,1) Not IN ('I', 'L') THEN a.[PtdBal11] ELSE a.[PtdBal11] * -1 END
      , PtdBal12 = CASE WHEN SubString(b.accttype, 2,1) Not IN ('I', 'L') THEN a.[PtdBal12] ELSE a.[PtdBal12] * -1 END
      ,a.[Sub]
