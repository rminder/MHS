      ,Convert(bigint, a.[tstamp]) as tstamp
      ,a.[TranDesc]
  FROM [APTran] a  WITH(NOLOCK) LEFT JOIN [APDoc] b  WITH(NOLOCK) ON
  a.BatNbr = b.BatNbr
 AND a.RefNbr = b.RefNbr
 AND a.VendID = b.VendID
  WHERE  Convert(bigint, a.[tstamp]) &gt; ?
  AND a.[Rlsed] = 1
  AND ((a.[TranType] = 'CK' AND a.DrCr = 'C') OR
